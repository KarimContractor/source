@model IEnumerable<WebApplication10.Models.Customers>

@{
    <h2>Customers List</h2>
    Layout = "~/Views/Shared/Adminpanel.cshtml";
}



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rating)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Customerfrom)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rating)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Customerfrom)
        </td>
        <td>
            @Html.ActionLink("Edit", "Customeredt", new { id=item.Id }) |
            
            @Html.ActionLink("Delete", "Customerdel", new { id=item.Id })
        </td>
    </tr>
}

</table>
