@model IEnumerable<WebApplication10.Models.Providers>

@{
    <h2>Providers List</h2>
    Layout = "~/Views/Shared/Adminpanel.cshtml";
}



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Rating)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Review)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Membersfrom)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Rating)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Review)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Membersfrom)
        </td>
        <td>
            @Html.ActionLink("Edit", "Provideredt", new { id=item.Id }) |
            
            @Html.ActionLink("Delete", "Providerdel", new { id=item.Id })
        </td>
    </tr>
}

</table>
